{
    "name": "stl-server",
    "version": "0.0.1",
    "description": "Server for STL",
    "main": "dist/index.js",
    "scripts": {
        "start": "nodemon dist/index.js",
        "dev": "nodemon src/index.ts",
        "ts.check": "tsc --project tsconfig.json",
        "build": "rimraf dist && tsc"
    },
    "author": "tgthuan2000",
    "license": "MIT",
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/lodash": "^4.14.191",
        "@types/node": "^18.13.0",
        "@types/node-schedule": "^2.1.0",
        "@types/nodemailer": "^6.4.7",
        "nodemon": "^2.0.20",
        "rimraf": "^4.1.2",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@sanity/client": "^5.2.1",
        "@sanity/uuid": "^3.0.1",
        "@types/cors": "^2.8.13",
        "@types/web-push": "^3.3.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "groq": "^3.4.0",
        "jsonwebtoken": "^9.0.0",
        "jwt-decode": "^3.1.2",
        "lodash": "^4.17.21",
        "node-schedule": "^2.1.1",
        "nodemailer": "^6.9.1",
        "web-push": "^3.5.0"
    },
    "engines": {
        "node": ">=16"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "node -r tsconfig-paths/register -r ts-node/register",
        "ext": "ts, js"
    }
}
